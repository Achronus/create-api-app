DATABASE_URL=mysql://<username>:<password>@localhost:3306/<app_name>
PROD_DATABASE_URL=
LOCAL_DATABASE_URL=mysql://<username>:<password>@localhost:3306/<app_name>

# https://www.builder.io/c/docs/using-your-api-key
NEXT_PUBLIC_BUILDER_API_KEY=
